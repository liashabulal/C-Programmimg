#include<stdio.h>
int main()
{
   int n,i,a[50];
   int key,flag=0;
   printf("Enter the number of elements in the array:\n");
   scanf("%d",&n);
   
   printf("Enter the elements of the array:\n");
   for(i=0;i<n;i++)
   {
       scanf("%d",&a[i]);
   }
   printf("Enter the search key:\n");
   scanf("%d",&key);
   for(i=0;i<n;i++)
   {
       if(key==a[i])
       {
           flag=1;
           break;
       }
   }
   if(flag==1)
   {
       printf("Key is found");
   }
   else
   {
       printf("Key is not found");
   }
}
